eprint = {1602.08112},
file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Cao et al. - 2016 - Low-temperature crystal and magnetic structure of $\alpha$-RuCl3.pdf:pdf},
isbn = {2469-9950},
