doi = {10.1038/nmat4604},
eprint = {1504.08037},
file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Banerjee et al. - 2016 - Proximate Kitaev quantum spin liquid behaviour in a honeycomb magnet.pdf:pdf},
isbn = {1476-4660},
issn = {14764660},
journal = {Nature Materials},
mendeley-groups = {Ruthenium Chloride},
number = {7},
pages = {733--740},
