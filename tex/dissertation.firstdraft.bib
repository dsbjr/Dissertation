mendeley-groups = {Dissertation references/Topological Order},
number = {2},
pages = {25--30},
title = {{Second-order phase transitions, L. Landau and his successors}},
url = {http://arxiv.org/abs/1102.1085},
volume = {3},
year = {2011}
}


