file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Han et al. - 2012 - Fractionalized excitations in the spin-liquid state of a kagome-lattice antiferromagnet.pdf:pdf},
issn = {00280836},
journal = {Nature},
mendeley-groups = {Dissertation references/Topological Order},
number = {7429},
pages = {406--410},
publisher = {Nature Publishing Group},
title = {{Fractionalized excitations in the spin-liquid state of a kagome-lattice antiferromagnet}},
url = {http://dx.doi.org/10.1038/nature11659},
volume = {492},
year = {2012}
}


@article{Chen2018,
abstract = {A one-parameter family of long-range resonating valence bond (RVB) state on the square lattice was previously proposed to describe a critical spin liquid (SL) phase of the spin-{\$}1/2{\$} frustrated Heisenberg model. We provide evidence that this RVB state in fact also realizes a topological (long-range entangled) {\$}\backslashmathbb{\{}Z{\}}{\_}2{\$} SL in a finite interval of parameter, limited by two transitions to critical SL phases. The topological phase is naturally connected to the {\$}\backslashmathbb{\{}Z{\}}{\_}2{\$} gauge symmetry of the local tensor. This work shows that, on one hand, spin-{\$}1/2{\$} topological SL with {\$}C{\_}{\{}4v{\}}{\$} point group symmetry and {\$}SU(2){\$} spin rotation symmetry exists on the square lattice and, on the other hand, criticality and nonbipartiteness are compatible. We also point out strong similarities between our phase diagram and the one of a classical interacting dimer model suggesting that both can be described by the same long-wavelength field theory. Incidentally, the entanglement spectrum is found to be gapped in the {\$}\backslashmathbb{\{}Z{\}}{\_}2{\$} phase.},
author = {Chen, Ji Yao and Poilblanc, Didier},
doi = {10.1103/PhysRevB.97.161107},
file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Chen, Poilblanc - 2018 - Topological Z 2 resonating-valence-bond spin liquid on the square lattice.pdf:pdf},
