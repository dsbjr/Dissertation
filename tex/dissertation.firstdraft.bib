doi = {10.1103/PhysRevB.95.174429},
file = {:C$\backslash$:/Users/dsbjr/Documents/Mendeley Desktop/Glamazda et al. - 2017 - Relation between Kitaev magnetism and structure in $\alpha$-RuCl 3.pdf:pdf},
