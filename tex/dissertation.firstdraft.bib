issn = {20403372},
journal = {Nanoscale},
mendeley-groups = {Ruthenium Chloride},
number = {40},
pages = {19014--19022},
title = {{Chemical vapor growth and delamination of $\alpha$-RuCl3 nanosheets down to the monolayer limit}},
volume = {10},
year = {2018}
}


